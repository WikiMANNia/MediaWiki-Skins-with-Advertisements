{
	"name": "Wima",
	"author": [
		"Gabriel Wicke",
		"Isarra Yos",
		"..."
	],
	"url": "https://www.mediawiki.org/wiki/Skin:Wima",
	"descriptionmsg": "wima-desc",
	"namemsg": "skinname-wima",
	"license-name": "GPL-2.0-or-later",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.29.0"
	},
	"ValidSkinNames": {
		"wima": "Wima"
	},
	"SkinOOUIThemes": {
		"wima": "Apex"
	},
	"DefaultUserOptions": {
		"wima-responsive": true
	},
	"Hooks": {
		"GetPreferences": "SkinWima::onGetPreferences",
		"ResourceLoaderRegisterModules": "SkinWima::registerMobileExtensionStyles"
	},
	"MessagesDirs": {
		"Wima": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.monobook.styles": {
			"class": "ResourceLoaderSkinModule",
			"features": [ "elements", "content", "interface", "logo", "legacy" ],
			"styles": {
				"resources/screen-common.less": {
					"media": "screen"
				},
				"resources/screen-desktop.css": {
					"media": "screen"
				}
			}
		},
		"skins.monobook.responsive": {
			"class": "ResourceLoaderSkinModule",
			"features": [
				"elements",
				"content",
				"interface",
				"legacy",
				"logo"
			],
			"targets": [
				"desktop",
				"mobile"
			],
			"styles": {
				"resources/screen-common.less": {
					"media": "screen"
				},
				"resources/screen-desktop.css": {
					"media": "screen and (min-width: 551px)"
				},
				"resources/screen-mobile.less": {
					"media": "screen and (max-width: 550px)"
				}
			}
		},
		"skins.monobook.mobile": {
			"class": "ResourceLoaderSkinModule",
			"features": [ "elements", "content", "interface", "logo", "legacy" ],
			"targets": [ "desktop", "mobile" ],
			"scripts": [
				"resources/mobile.js"
			],
			"styles": {
				"resources/mobile.js.less": {
					"media": "screen and (max-width: 550px)"
				}
			},
			"dependencies": [
				"mediawiki.util"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Wima"
	},
	"ResourceModuleSkinStyles": {
		"monobook": {
			"+mediawiki.action.view.redirectPage": "resources/skinStyles/mediawiki.action.view.redirectPage.css",
			"+mediawiki.notification": "resources/skinStyles/mediawiki.notification.less",
			"+oojs-ui-core.styles": "resources/skinStyles/ooui.less",
			"+ext.uls.interface": "resources/skinStyles/ext.uls.interface.less",
			"+ext.relatedArticles.readMore": "resources/skinStyles/ext.relatedArticles.readMore.less"
		}
	},
	"AutoloadClasses": {
		"SkinWima": "includes/SkinWima.php",
		"WimaTemplate": "includes/WimaTemplate.php"
	},
	"manifest_version": 2
}
